{
  "plugins": [
    ["lodash", {"id": ["lodash"]}],
    "dynamic-import-node",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "routes": "./src/routes",
          "common": "./src/common"
        },
        "extensions": [".js", ".jsx", ".json"]
      }
    ]
  ],
  "presets": [
    ["env", {"targets": "node"}],
    "flow"
  ]
}
